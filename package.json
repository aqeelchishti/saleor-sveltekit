{
    "name": "saleor-sveltekit",
    "version": "0.0.2",
    "author": "Aqeel Chishti",
    "scripts": {
        "dev:only": "vite dev",
        "build:only": "vite build",
        "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
        "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
        "dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\"",
        "build": "npm run tailwind:build && npm run build:only",
        "gen": "houdini generate",
        "preview": "vite preview",
        "package": "vite package",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "devDependencies": {
        "@stripe/stripe-js": "^1.31.0",
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/kit": "next",
        "@types/nprogress": "^0.2.0",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "autoprefixer": "10.4.5",
        "concurrently": "^7.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "graphql": "^15.8.0",
        "houdini": "^0.17.13",
        "houdini-svelte": "^0.17.13",
        "postcss": "^8.4.13",
        "postcss-cli": "^9.1.0",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.5.0",
        "stripe": "^9.5.0",
        "svelte": "^3.44.0",
        "svelte-check": "^2.2.6",
        "svelte-preprocess": "^4.10.1",
        "svelte-stripe": "^0.0.17",
        "tailwind-scrollbar": "^1.3.1",
        "tailwindcss": "^3.0.24",
        "tslib": "^2.3.1",
        "vite": "^3.2.4"
    },
    "type": "module",
    "dependencies": {
        "@graphql-yoga/render-graphiql": "^3.0.3",
        "@kitql/all-in": "^0.8.2",
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/forms": "^0.4.1",
        "@tailwindcss/typography": "^0.5.2",
        "daisyui": "^2.14.3",
        "graphql": "^15.8.0",
        "houdini": "^0.17.13",
        "nprogress": "^0.2.0",
        "svelte-skeleton": "^1.3.1"
    }
}